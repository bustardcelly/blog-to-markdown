<article id="archives">
  <h1>archives</h1>
  <ul>
  <% _(site.posts).chain().reverse().each(function(post){ %>
    <li>
      <p>
        <a href="<%= rootlocation %>/<%= post.htmlPath() %>"><%= post.title() %></a>
        <br/>
        <span class="archive-date"><%= post.date() %></span>
      </p>
    </li>
  <% }) %>
  </ul>
</article>